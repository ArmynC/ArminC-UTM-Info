CFLAGS = -Wall

.PHONY: all clean

all: extern-1 extern-2 extern-3 extern-4 extern-5

extern-1: extern-1.o

extern-1.o: extern-1.c

extern-2: extern-2.o

extern-2.o: extern-2.c

extern-3: extern-3.o

extern-3.o: extern-3.c

extern-4: extern-4.o

extern-4.o: extern-4.c

extern-5: extern-5.o

extern-5.o: extern-5.c

clean:
	-rm -f extern-1 extern-2 extern-3 extern-4 extern-5 *.o
	-rm -f *~
